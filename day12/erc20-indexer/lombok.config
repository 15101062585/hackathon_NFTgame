# 基础Lombok配置 - 为Trae开发工具优化

# 启用链式访问器
lombok.accessors.chain=true

# 让equals和hashCode方法调用父类方法
lombok.equalsAndHashCode.callSuper=call

# 日志相关配置
lombok.log.fieldName=log
lombok.log.slf4j.enable=true
lombok.log.fieldIsStatic=true
lombok.log.fieldIsFinal=true

# 禁用实验性功能以提高兼容性
lombok.experimental=false

# 禁用空值注解以减少冲突
lombok.addNullAnnotations=none

# 配置构造函数生成策略
lombok.anyConstructor.addConstructorProperties=true

# 允许使用静态导入lombok注解
lombok.imports=static

# 确保日志字段可见性
lombok.log.fieldVisibility=private